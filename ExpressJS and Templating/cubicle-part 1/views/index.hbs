<main>
  <h1>Browser</h1>
  <form action="/search" method="GET">
    <input type="text" class="search" name="search" placeholder="Search...">
    <input type="number" class="difficulty" name="from" placeholder="Difficulty from...">
    <span>-</span>
    <input type="number" class="difficulty" name="to" placeholder="Difficulty to...">
    <input type="submit" value="search">
  </form>

  {{#each cubes}}
  {{> cube}}
  {{else}}
  <h1>NO CUBES WERE FOUND</h1>
  {{/each}}

</main>